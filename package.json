{
  "name": "Astral",
  "private": true,
  "scripts": {
    "test": "karma start --singleRun=true",
    "lint": "eslint resources/assets/js --ext='.js,.vue'",
    "clean": "rm -f public/build/css/* public/build/js/* public/build/rev-manifest.json",
    "clean-deps": "rm -rf node_modules",
    "reinstall-deps": "npm run clean-deps && npm install",
    "dev": "cross-env NODE_ENV=development webpack --progress --hide-modules",
    "watch": "cross-env NODE_ENV=development webpack --watch --progress --hide-modules",
    "hot": "cross-env NODE_ENV=development webpack-dev-server --inline --hot",
    "production": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "dependencies": {
    "dragula": "^3.6.3",
    "es6-promise": "^4.0.5",
    "highlight.js": "^9.5.0",
    "jquery": "^2.2.0",
    "js-base64": "^2.1.9",
    "local-storage": "^1.4.2",
    "lodash": "^4.0.0",
    "marked": "^0.3.5",
    "select2": "^4.0.1",
    "simplemde": "^1.11.2",
    "vue": "^2.1.10",
    "vue-clickaway": "^2.1.0",
    "vue-resource": "^0.6.1",
    "vue-router": "^2.1.3",
    "vuex": "^2.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-runtime": "^6.9.0",
    "eslint": "^3.1.1",
    "eslint-config-vue": "^1.0.3",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-vue": "^0.1.1",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.19",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.3",
    "karma-spec-reporter": "0.0.23",
    "laravel-mix": "^0.8.8",
    "node-bourbon": "^4.2.3",
    "phantomjs": "^1.9.19"
  }
}
